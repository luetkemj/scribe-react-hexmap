{
  "name": "@scribe/react-hexmap",
  "version": "v0.1.1-alpha",
  "description": "A Fantasy RPG Hexmap component built with React",
  "repository": {
    "type": "git",
    "url": "https://github.com/luetkemj/scribe-react-hexmap.git"
  },
  "author": "luetkemj",
  "license": {
    "type": "MIT",
    "url": "https://github.com/luetkemj/scribe-hexmap/raw/master/LICENSE"
  },
  "main": "build/index.js",
  "scripts": {
    "build": "npm run clean:build && npm run clean:dist && npm run lint && npm run test && npm run webpack && babel src -d build",
    "clean:build": "rimraf build",
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "clean:node_modules": "rimraf node_modules",
    "clean": "npm run clean:build && npm run clean:node_modules",
    "reinstall": "npm run clean && npm install",
    "lint:eslint": "eslint .",
    "lint": "npm run lint:eslint",
    "prepublish": "npm run build",
    "test:watch": "jest --watch --coverage",
    "test": "npm run lint && jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "webpack": "./node_modules/.bin/webpack --config webpack.config.js"
  },
  "jest": {
    "verbose": false
  },
  "dependencies": {
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "lodash": "^4.17.4",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-hexgrid": "1.0.0-rc4"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-glamor": "3.0.0-1",
    "babel-plugin-transform-strict-mode": "6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.24.1",
    "css-loader": "^0.28.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-airbnb-base": "11.1.3",
    "eslint-plugin-import": "2.2.0",
    "jest": "19.0.2",
    "node-sass": "^4.5.2",
    "rimraf": "2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1"
  }
}
